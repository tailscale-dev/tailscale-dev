{
  "name": "tailscale-dev",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "next dev",
    "dev": "next dev",
    "build": "next build && yarn build-postbuild && node dist/postbuild.js",
    "build-postbuild": "esbuild scripts/postbuild.ts --bundle --platform=node --outfile=dist/postbuild.js",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "precommit": "lint-staged",
    "lint": "next lint --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@code-hike/mdx": "^0.8.0",
    "@next/bundle-analyzer": "13.0.2",
    "@next/font": "13.0.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "autoprefixer": "^10.4.13",
    "contentlayer": "0.2.8",
    "date-fns": "^2.30.0",
    "esbuild": "^0.17.4",
    "github-slugger": "^1.4.0",
    "gray-matter": "^4.0.2",
    "image-size": "1.0.0",
    "mdx-bundler": "^9.0.0",
    "next": "^13.1.5",
    "next-contentlayer": "0.2.8",
    "next-themes": "^0.2.0",
    "postcss": "^8.4.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-preset-minify": "6.0.0",
    "rehype-slug": "^5.0.0",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "sharp": "^0.31.3",
    "tailwindcss": "^3.2.2",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.3.1",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@types/mdx": "^2.0.3",
    "@types/react": "^18.0.18",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.23.0",
    "eslint-config-next": "13.0.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-unicorn": "^46.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "file-loader": "^6.0.0",
    "globby": "11.0.3",
    "html-loader": "^4.2.0",
    "husky": "^8.0.3",
    "js-yaml": "4.1.0",
    "lint-staged": "^13.0.0",
    "prettier": "^2.7.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "raw-loader": "^4.0.2",
    "schema-dts": "^1.1.2",
    "typescript": "^4.8.3"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": "~18"
  }
}
