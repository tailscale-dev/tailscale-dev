{
  "name": "tailscale-dev",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "next dev",
    "dev": "next dev",
    "build": "next build && yarn build-postbuild && node dist/postbuild.js",
    "build-postbuild": "esbuild scripts/postbuild.ts --bundle --platform=node --outfile=dist/postbuild.js",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "precommit": "lint-staged",
    "lint": "next lint --fix",
    "prepare": "husky install",
    "ci:indexjanitor": "node ./scripts/indexjanitor.js",
    "ci:searchindex": "ts-node ./scripts/searchindex.ts"
  },
  "dependencies": {
    "@code-hike/mdx": "^0.8.0",
    "@elastic/elasticsearch": "^8.9.0",
    "@next/bundle-analyzer": "13.0.2",
    "@next/font": "13.0.2",
    "@vercel/analytics": "^1.0.2",
    "autoprefixer": "^10.4.15",
    "contentlayer": "0.2.8",
    "esbuild": "^0.19.2",
    "execa": "^7.2.0",
    "github-slugger": "^1.5.0",
    "image-size": "1.0.0",
    "next": "^13.1.5",
    "next-contentlayer": "0.2.8",
    "next-themes": "^0.2.0",
    "postcss": "^8.4.28",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-preset-minify": "6.0.0",
    "rehype-slug": "^6.0.0",
    "remark": "^14.0.3",
    "remark-gfm": "^3.0.1",
    "tailwindcss": "^3.3.3",
    "unist-util-visit": "^4.1.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/mdx": "^2.0.3",
    "@types/react": "^18.0.18",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "eslint-config-next": "13.0.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "globby": "11.1.0",
    "husky": "^8.0.3",
    "js-yaml": "4.1.0",
    "lint-staged": "^14.0.0",
    "prettier": "^2.8.8",
    "schema-dts": "^1.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vfile": "^5.3.7"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": "~18"
  }
}
